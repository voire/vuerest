{
  "name": "@voire/vuerest",
  "description": "Set of composables for RESTful API calls in Clean architecture.",
  "version": "0.0.0-development",
  "license": "MIT",
  "type": "module",
  "author": "Pavel Yankovski <https://github.com/pyncz>",
  "keywords": [
    "typescript",
    "vue",
    "vue3",
    "vue-next",
    "composable"
  ],
  "scripts": {
    "build": "nr gen:exports && unbuild && nr cleanup",
    "gen:types": "esno scripts/genTypes.ts",
    "gen:exports": "esno scripts/genExports.ts",
    "cleanup": "esno scripts/cleanUp.ts",
    "semantic-release": "semantic-release"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@types/node": "^18.0.6",
    "@voire/type-utils": "^1.1.0",
    "@vueuse/core": "^8.9.4",
    "ohmyfetch": "^0.4.18",
    "ufo": "^0.8.5",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@antfu/ni": "^0.17.2",
    "esno": "^0.16.3",
    "semantic-release": "^19.0.2",
    "unbuild": "^0.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/voire/vuerest.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
